{
  "name": "oda-model",
  "version": "1.0.0-30",
  "description": "",
  "main": "dist/model/index.js",
  "typescript": {
    "definition": "dist/model/index.d.ts"
  },
  "typings": "dist/model/index.d.ts",
  "scripts": {
    "test": "jest",
    "prepare": "npm run clean && npm run compile",
    "compile": "tsc -p ./",
    "wipe": "./clean.sh",
    "watch": "tsc -w -p ./",
    "clean": "rm -rf dist",
    "lint": "tslint -c \"../../config/tslint.json\" -p tsconfig.json src/*.ts"
  },
  "author": "Alex Vedmdedenko <vedmalex@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "camelcase": "^5.0.0",
    "decamelize": "^2.0.0",
    "global": "^4.3.2",
    "inflected": "^2.0.3",
    "oda-isomorfic": "file:../oda-isomorfic"
  },
  "peerDependencies": {
    "lodash": "^4.17.10"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "devDependencies": {
    "typescript": "^3.0.1"
  }
}
