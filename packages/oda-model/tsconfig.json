{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es2018",
    "lib": [
      "es2017",
      "esnext.asynciterable"
    ],
    "baseUrl": ".",
    "sourceMap": true,
    "declaration": true,
    "noImplicitAny": false,
    "rootDir": "src",
    "outDir": "dist",
    "allowSyntheticDefaultImports": true,
    "removeComments": true,
    "pretty": true,
    "paths": {
      "@validator/*":["src/model/validator/*"],
      "@model/*":["src/model/*"]
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
