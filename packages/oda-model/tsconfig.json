{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es5",
    "lib": [
      "es2017",
      "esnext.asynciterable"
    ],
    "baseUrl": ".",
    "downlevelIteration": true,
    "sourceMap": true,
    "declaration": true,
    "noImplicitAny": false,
    "rootDir": "src",
    "outDir": "dist",
    "allowSyntheticDefaultImports": true,
    "removeComments": true,
    "pretty": true
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
