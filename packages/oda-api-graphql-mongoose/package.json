{
  "name": "oda-api-graphql-mongoose",
  "version": "1.0.0-17",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "prepare": "npm run clean && npm run compile",
    "compile": "tsc -p ./",
    "wipe": "./clean.sh",
    "clean": "rm -rf dist",
    "lint": "tslint -c \"../../config/tslint.json\" -p tsconfig.json src/*.ts"
  },
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "typings": "dist/index.d.ts",
  "author": "Alex Vedmdedenko <vedmalex@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "oda-api-graphql": "file:../oda-api-graphql",
    "oda-isomorfic": "file:../oda-isomorfic"
  },
  "peerDependencies": {
    "graphql": "^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0 || ^0.13.0",
    "graphql-tag": "^2.9.2",
    "log4js": "^1.1.1",
    "mongoose": "^5.2.9",
    "muri": "^1.3.0"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "devDependencies": {
    "typescript": "^3.0.1"
  }
}
