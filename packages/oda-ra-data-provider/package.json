{
  "name": "oda-ra-data-provider",
  "version": "1.0.0-15",
  "main": "dist/index.js",
  "author": "Alex Vedmdedenko <vedmalex@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "prepare": "npm run clean && npm run compile",
    "compile": "tsc -p ./",
    "wipe": "./clean.sh",
    "clean": "rm -rf dist",
    "lint": "tslint -c \"../../config/tslint.json\" -p tsconfig.json src/*.ts"
  },
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "typings": "dist/index.d.ts",
  "dependencies": {
    "comparator.js": "0.0.11",
    "oda-isomorfic": "file:../oda-isomorfic",
    "oda-lodash": "file:../oda-lodash"
  },
  "peerDependencies": {
    "ra-core": "^2.2.1",
    "redux-form": "^7.4.2",
    "redux-saga": "^0.16.0",
    "apollo-cache-inmemory": "^1.1.12",
    "apollo-client": "^2.2.8",
    "apollo-link": "^1.1.0",
    "apollo-link-http": "^1.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "devDependencies": {
    "typescript": "^3.0.1"
  }
}
