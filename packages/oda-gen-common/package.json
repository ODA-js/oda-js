{
  "name": "oda-gen-common",
  "version": "1.0.0-17",
  "main": "dist/index.js",
  "author": "Alex Vedmdedenko <vedmalex@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "prepare": "npm run clean && npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "wipe": "./clean.sh",
    "clean": "rm -rf dist",
    "lint": "tslint -c \"../../config/tslint.json\" -p tsconfig.json src/*.ts"
  },
  "typescript": {
    "definition": "dist/index.d.ts"
  },
  "typings": "dist/index.d.ts",
  "dependencies": {
    "immutable": "^4.0.0-rc.9",
    "invariant": "^2.2.2",
    "oda-isomorfic": "file:../oda-isomorfic",
    "warning": "^3.0.0"
  },
  "peerDependencies": {
    "graphql": "^0.11.0 || ^0.12.0 || ^0.13.0",
    "graphql-tag": "^2.9.2",
    "graphql-tools": "^3.0.4",
    "lodash": "^4.17.10"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "devDependencies": {
    "apollo-server-core": "^2.0.4",
    "jest": "^23.5.0",
    "ts-jest": "^23.1.4",
    "typescript": "^3.0.1"
  }
}
